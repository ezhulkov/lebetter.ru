<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">

    <head>
        <title>${title}</title>
        <meta http-equiv="content-type" content="text/html; charset=utf-8"/>
        <meta name="viewport" content="width=device-width, minimum-scale=1.0, maximum-scale=1.0"/>

        <t:if test="keywords">
            <meta name="keywords" content="${keywords}"/>
        </t:if>
        <t:if test="description">
            <meta name="description" content="${description}"/>
        </t:if>

		<link rel="stylesheet" href="/styles/galleriffic.css" type="text/css" />
		<script type="text/javascript" src="/scripts/jquery.galleriffic.js"></script>
		<script type="text/javascript" src="/scripts/jquery.opacityrollover.js"></script>

        <script type="text/javascript" language="JavaScript">
            jQuery.noConflict();
        </script>
    </head>
    <body id="${bodyId}">

        <table class="body">
            <tr>
                <td class="top">

                    <!--noindex-->
                    <t:mallcomponents.layout.header/>
                    <!--/noindex-->

                    <div class="content">
                        <div class="centered">

                            <t:body/>

                        </div>
                    </div>

                </td>
            </tr>
            <tr>
                <td class="bottom">

                    <!--noindex-->
                    <t:mallcomponents.layout.footer/>
                    <!--/noindex-->

                </td>
            </tr>
        </table>

        <!--noindex-->
        <!-- PrintVersion -->
        <div class="printable"></div>

        <t:mallComponents.control.javascript>
              /* Start 'topmenu' */
              AG.menuPlay();
        </t:mallComponents.control.javascript>

        <t:mallComponents.control.javascriptDestination/>
        <!--/noindex-->

    </body>
</html>
